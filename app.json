    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lamjq.IChing"
    },
    "web": {
      "favicon": "./app/assets/logo/favicon.png"
    },
    "android": {
      "package": "com.lamjq.IChing"
    },
    "description": "This is the IChing App"
  }
}
