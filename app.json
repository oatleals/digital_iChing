{
  "expo": {
    "name": "IChing",
    "slug": "iching",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/logo/icon.png",
    "splash": {
      "image": "./app/assets/logo/IchingKoiLogo.png",
      "resizeMode": "contain",
      "width": 100,
      "height": 100,
      "backgroundColor": "#008080"
